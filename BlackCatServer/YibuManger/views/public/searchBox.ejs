<!--搜索框-->
<form action="<%=currentLink%>" name="searchForm" >
    <div class="input-group">
        <input type="text" name="searchKey" id="searchInput" class="form-control input-sm pull-right"
               style="width: 200px; margin-top: 7px; float: left"
               placeholder="请输入需要查询的关键字" value="<%=searchKey%>"/>
        <button class="btn btn-sm btn-default" type="submit"><i class=" icon-search "></i></button>
    </div>
</form>